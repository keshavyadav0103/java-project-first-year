<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/appback"
    tools:context=".encrypt">


    <Button
        android:id="@+id/starte"
        android:layout_width="351dp"
        android:layout_height="81dp"
        android:text="Start Encryption"
        android:textColor="@color/black"
        android:backgroundTint="@color/white"
        android:onClick="starte"
        android:textSize="24dp"
        app:cornerRadius="30dp"

        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.233" />

    <Button
        android:id="@+id/getkey"
        android:layout_width="351dp"
        android:layout_height="81dp"
        android:backgroundTint="@color/white"
        android:onClick="getkey"
        android:text="GET KEY"
        android:textColor="@color/black"
        android:textSize="24dp"
        app:cornerRadius="30dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.556" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="100dp"
        android:layout_height="86dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.948"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.947"
        app:srcCompat="@drawable/baseline_download_24" />

    <TextView
        android:id="@+id/gkey"
        android:layout_width="291dp"
        android:layout_height="126dp"
        android:text=" "
        android:textColor="@color/white"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.125"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.777" />

    <TextView
        android:id="@+id/en"
        android:layout_width="409dp"
        android:layout_height="93dp"
        android:text=" "
        android:textColor="@color/white"
        tools:layout_editor_absoluteX="5dp"
        tools:layout_editor_absoluteY="296dp" />

    <ImageView
        android:id="@+id/copy_key"
        android:layout_width="89dp"
        android:layout_height="70dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.95"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.761"
        app:srcCompat="@drawable/baseline_content_copy_24" />
</androidx.constraintlayout.widget.ConstraintLayout>